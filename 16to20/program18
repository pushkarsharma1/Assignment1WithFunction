//WAP to print all the prime numbers from 1 to 300.

#include<stdio.h>
void checkPrime();
int main(){
    checkPrime();
}

void checkPrime(){
    int i,j,prime;
    for(i=1;i<=300;i++){
        prime=0;
        for(j=2;j<i;j++){
            if(i%j==0){
                prime =1;
                break;
            }
        }
        if(prime==0 && i!=1){
            printf("%d\t",i);
        }
    }

}
